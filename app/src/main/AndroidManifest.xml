<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.olle.androidgame">

    <uses-permission android:name="android.permission.INTERNET" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/rabbid_rabbits_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".game.GameView" />
        <activity
            android:name=".game.GameActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".highscore.HighScoreActivity"
            android:label="@string/title_activity_high_score"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".credits.CreditsActivity"
            android:label="@string/title_activity_credits"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".map.MapActivity"
            android:label="@string/title_activity_map"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>